{
  "name": "olive-gold-panda",
  "version": "1.0.1",
  "description": "A game for the Global Game Jam 2024. Codenamed Olive Gold Panda as generated by the random generator. Made with rayjs",
  "type": "module",
  "scripts": {
    "play-lin": "./dist/rayjs-linux ./dist/main.js",
    "play-win": ".\\dist\\rayjs.exe .\\dist\\main.js",
    "play-mac": "./dist/rayjs-mac ./dist/main.js",
    "dev": "webpack --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js",
